{
  "name": "mike-bifulco-blog",
  "description": "Just a place for me to put it all out into the world",
  "version": "0.1.0",
  "author": "Mike Bifulco <mbifulco@live..com>",
  "keywords": [
    "gatsby",
    "minimal",
    "starter",
    "blog",
    "theme",
    "dark",
    "light",
    "personal site"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "start": "gatsby clean && gatsby develop",
    "dev": "gatsby clean && gatsby develop",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
