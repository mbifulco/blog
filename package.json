    "@material-ui/lab": "^4.0.0-alpha.56",
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@weknow/gatsby-remark-twitter": "^0.2.3",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.24.29",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^2.4.14",
