    "takeshape-routing": "^5.0.4",
    "use-cloudinary": "^3.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.5.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
